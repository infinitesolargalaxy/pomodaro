{
    "manifest_version": 2,
    "name": "Pomodaro",
    "version": "0.1",
    "description": "Pomodaro Extension!",
    "browser_action": {
      "default_icon": "./assets/gallery.png",
      "default_popup": "popup/index.html"
    },
    "icons": {
    },
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "http://*/",
      "https://*/"
    ],
    "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScripts/index.tsx"],
      "css": ["style.css","reset.css","normalize.css"]
    }],
    "web_accessible_resources": [
      "./assets/Alert.mp3",
      "./assets/alice.jpg",
      "./assets/koga.jpg"
    ]
  }